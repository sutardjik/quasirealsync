    <Grid>
        <Border Height="800"
                Width="1200"
                CornerRadius="25"
                Background="#524E4F">
            <Border.Effect>
                <DropShadowEffect Color="#000000"
                                  Opacity="0.5"
                                  Direction="90"
                                  ShadowDepth="1"
                                  BlurRadius="35"/>
            </Border.Effect>
            <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="901,170,75,376"/>
            
        </Border>
        <Border Height="40"
                Width="1020"
                Background="#363435"
                BorderThickness="3"
                VerticalAlignment="Top"
                HorizontalAlignment="Center"
                Margin="0,75,0,0"
                CornerRadius="5"/>
        <Menu Height="30"
              Width="1010"
              Background="#363435"
              VerticalAlignment="Top"
              HorizontalAlignment="Center"
              Margin="0,80,0,0">
